<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magenest_MapList::main" title="Google Map List and Store Locator" sortOrder="100">
                    <resource id="Magenest_MapList::list_map" title="Map List" sortOrder="10" />
                    <resource id="Magenest_MapList::list_location" title="Locations Details" sortOrder="20" />
                    <!--<resource id="Magenest_MapList::category" title="Tags" sortOrder="30" />-->
                    <resource id="Magenest_MapList::config" title="Configurations" sortOrder="40"/>
                </resource>

                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_MapList::maplist_config" title="Map List" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>