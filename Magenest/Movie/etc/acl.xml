<?xml    version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_Movie::config_movie" title="Movie Section"/>
                        </resource>
                    </resource>
                </resource>

                <resource id="Magenest_Movie::movie" title="Movie Table">
                    <resource id="Magenest_Movie::gird" title="Magenest Grid">
                        <resource id="Magenest_Movie::magenestmovie" title="Magenest Movie"/>
                        <resource id="Magenest_Movie::magenestdirector" title="Magenest Director"/>
                        <resource id="Magenest_Movie::magenestactor" title="Magenest Actor"/>
                    </resource>
                </resource>

                <resource id="Magenest_Movie::requestreport" title="Request Report">
                    <resource id="Magenest_Movie::request" title="Request" />
                </resource>

            </resource>
        </resources>
    </acl>
</config>